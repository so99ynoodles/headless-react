    "@react-aria/overlays": "^3.9.1",
    "@react-aria/utils": "^3.13.1",
    "@react-stately/collections": "^3.4.1",
    "@react-stately/combobox": "^3.1.1",
    "@types/react": "18.0.14"
  },
  "peerDependencies": {
