{
  "name": "@react-headless/combobox",
  "description": "React headless components",
  "version": "0.0.1",
  "source": "src/index.ts",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
